<!--CATEGORIES-->

{{#each categories}}
<div class="categories-wrapper">
  <div class="categories">
    <div class="category">
      <div class="left">
        <img src="{{this.icon}}" alt="user">
        <div class="content">
          <h1><a href="/categories/{{this.id}}">{{this.name}}</a></h1>
          
        </div>
      </div>
      <div class="options">
        <div class="toggle-btn">
          <img src="/images/icons8-options-50.png" alt="option">
        </div>
      </div>
    </div>
  </div>
</div>
{{/each}}

