<!--CATEGORIES-->


<div class="categories-wrapper">
  <div class="categories">
    {{#each categories}}
    <div class="category">
      <div class="left">
        <img src="{{this.icon}}" alt="user">
        <div class="content">
          <h1><a href="/categories/{{this.id}}">{{this.name}}</a></h1>
          
        </div>
      </div>
      
    </div>
    {{/each}}
  </div>
</div>


