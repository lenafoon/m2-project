<div class="category-screen">
    <div class="back-btn">
      <a href="/" class="back-btn">
      <img src="/images/icons8-back-64.png" alt="Back" />
      </a>
</div>

 <div class="category-details">
        <img src="/images/icons8-medal-64.png" alt="" id="medal">
        <div class="details">
            <h1>Challenges</h1>
        </div>
    </div>

{{#each challenges}}
  <div class="categories-wrapper">
    <div class="categories">
      <div class="category">
        <img src="{{this.icon}}" alt="icon">

        <div class="content">
          <h1><a href="/challenge/{{this.name}}">{{this.name}}</a></h1>
        </div>
      </div>
    </div>
  </div>
{{/each}}

</div>