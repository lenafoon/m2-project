<h1>Challenges</h1>
<ul>
  {{#each challenges}}
    <li>
      <strong>Name:</strong> {{this.name}}<br>
      <strong>Difficulty:</strong> {{this.difficulty}}<br>
      <strong>Completed:</strong> {{#if this.completed}}Yes{{else}}No{{/if}}<br>
      <a href="/challenges/{{name}}">View Details</a>
    </li>
  {{/each}}
</ul>
